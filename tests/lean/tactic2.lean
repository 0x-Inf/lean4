Variables p q r : Bool

Theorem T1 : p => q => p /\ q :=
   Discharge (fun H1,
      Discharge (fun H2,
            let H1 : p := _,
                H2 : q := _
            in Conj H1 H2
         )).
    assumption (* solve first metavar *)
    done
    apply assumption_tac (* solve second metavar *)
    done

(**
simple_tac = REPEAT(imp_tac() ^ conj_tac() ^ assumption_tac())
