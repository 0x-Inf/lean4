  Set: pp::unicode
  Imported 'cast'
  Set: pp::colors
λ (A A' : TypeM) (a : A) (b : A') (L2 : A' == A), let b' : A := cast L2 b, L3 : b == b' := cast::eq L2 b in L3 :
    ∀ (A A' : TypeM) (a : A) (b : A') (L2 : A' == A), b == cast L2 b
λ (A A' : TypeM)
  (B : A → TypeM)
  (B' : A' → TypeM)
  (f : ∀ x : A, B x)
  (g : ∀ x : A', B' x)
  (a : A)
  (b : A')
  (H2 : f == g)
  (H3 : a == b),
  let L1 : A == A' := type::eq H3,
      L2 : A' == A := symm L1,
      b' : A := cast L2 b,
      L3 : b == b' := cast::eq L2 b,
      L4 : a == b' := htrans H3 L3,
      L5 : f a == f b' := congr2 f L4
  in L5 :
    ∀ (A A' : TypeM)
      (B : A → TypeM)
      (B' : A' → TypeM)
      (f : ∀ x : A, B x)
      (g : ∀ x : A', B' x)
      (a : A)
      (b : A')
      (H2 : f == g)
      (H3 : a == b),
      f a == f (cast (symm (type::eq H3)) b)
